CREATE TABLE `t_trade` (
	`TradingDay` VARCHAR(9) NULL DEFAULT NULL COMMENT '交易日',
	`SettlementGroupID` VARCHAR(9) NULL DEFAULT NULL COMMENT '结算组代码',
	`SettlementID` INT(11) NULL DEFAULT NULL COMMENT '结算编号',
	`TradeID` VARCHAR(13) NULL DEFAULT NULL COMMENT '成交编号',
	`Direction` INT(11) NULL DEFAULT NULL COMMENT '买卖方向',
	`OrderSysID` VARCHAR(13) NULL DEFAULT NULL COMMENT '报单编号',
	`ParticipantID` VARCHAR(11) NULL DEFAULT NULL COMMENT '会员代码',
	`ClientID` VARCHAR(11) NULL DEFAULT NULL COMMENT '客户代码',
	`TradingRole` INT(11) NULL DEFAULT NULL COMMENT '交易角色',
	`AccountID` VARCHAR(13) NULL DEFAULT NULL COMMENT '资金帐号',
	`InstrumentID` VARCHAR(31) NULL DEFAULT NULL COMMENT '合约代码',
	`OffsetFlag` INT(11) NULL DEFAULT NULL COMMENT '开平标志',
	`HedgeFlag` INT(11) NULL DEFAULT NULL COMMENT '投机套保标志',
	`Price` DOUBLE(12,3) NULL DEFAULT NULL COMMENT '价格',
	`Volume` INT(11) NULL DEFAULT NULL COMMENT '数量',
	`TradeTime` VARCHAR(9) NULL DEFAULT NULL COMMENT '成交时间',
	`TradeType` INT(11) NULL DEFAULT NULL COMMENT '成交类型',
	`PriceSource` INT(11) NULL DEFAULT NULL COMMENT '成交价来源',
	`UserID` VARCHAR(16) NULL DEFAULT NULL COMMENT '交易用户代码',
	`OrderLocalID` VARCHAR(13) NULL DEFAULT NULL COMMENT '本地报单编号',
	`ClearingPartID` VARCHAR(11) NULL DEFAULT NULL COMMENT '结算会员编号',
	`BusinessUnit` VARCHAR(21) NULL DEFAULT NULL COMMENT '业务单元'
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;
