create table t_order(
TradingDay         	varchar(9)		comment		'交易日'             	,
SettlementGroupID  	varchar(9)		comment   '结算组代码'          ,
SettlementID       	int						comment       '结算编号'            ,
OrderSysID         	varchar(13)		comment   '报单编号'            ,
ParticipantID      	varchar(11)		comment   '会员代码'            ,
ClientID           	varchar(11)		comment   '客户代码'            ,
UserID             	varchar(16)		comment   '交易用户代码'        ,
InstrumentID       	varchar(31)		comment   '合约代码'            ,
OrderPriceType     	int						comment       '报单价格条件'        ,
Direction          	int						comment       '买卖方向'            ,
CombOffsetFlag     	varchar(5)		comment   '组合开平标志'        ,
CombHedgeFlag      	varchar(5)		comment   '组合投机套保标志'    ,
LimitPrice         	double(12,3)	comment   '价格'                ,
VolumeTotalOriginal	int						comment       '数量'                ,
TimeCondition      	int						comment       '有效期类型'          ,
GTDDate            	varchar(9)		comment   'TD日期'              ,
VolumeCondition    	int						comment       '成交量类型'          ,
MinVolume          	int						comment       '最小成交量'          ,
ContingentCondition	int						comment       '触发条件'            ,
StopPrice          	double(12,3)	comment   '止损价'              ,
ForceCloseReason   	int						comment       '强平原因'            ,
OrderLocalID       	varchar(13)		comment   '本地报单编号'        ,
IsAutoSuspend      	int						comment       '自动挂起标志'        ,
OrderSource        	int						comment       '报单来源'            ,
OrderStatus        	int						comment       '报单状态'            ,
OrderType          	int						comment       '报单类型'            ,
VolumeTraded       	int						comment       '今成交数量'          ,
VolumeTotal       	int						comment       '剩余数量'            ,
InsertDate         	varchar(9)		comment   '报单日期'            ,
InsertTime         	varchar(9)		comment   '插入时间'            ,
ActiveTime         	varchar(9)		comment   '激活时间'            ,
SuspendTime        	varchar(9)		comment   '挂起时间'            ,
UpdateTime         	varchar(9)		comment   '最后修改时间'        ,
CancelTime         	varchar(9)		comment   '撤销时间'            ,
ActiveUserID       	varchar(16)		comment   '最后修改交易用户代码',
Priority            int						comment       '优先权'              ,
TimeSortID         	int						comment       '按时间排队的序号'    ,
ClearingPartID     	varchar(11)		comment   '结算会员编号'        ,
BusinessUnit       	varchar(21)		comment   '业务单元'            )

